   ],
   "source": [
    "x, y, a, y1, y2, theta, v, m, g  = sp.symbols('x y a y1 y2 θ v m g')\n",
    "L = sp.symbols('L', cls=sp.Function)\n",
    "\n",
    "v = sp.solve(1/2 * m *v**2 - m*g*y, v)[1]  # from conservation of energy (keep positive solution)\n",
    "v = sp.sqrt(2-y)\n",
    "ds = sp.sqrt(1+y1**2)\n",
    "L = ds/v\n",
    "L"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
